{
    "name": "spatie/laravel-csp",
    "description": "Add CSP headers to the responses of a Laravel app",
    "keywords": [
        "spatie",
        "laravel-csp",
        "content-security-policy",
        "security",
        "headers",
        "csp",
        "laravel"
    ],
    "homepage": "https://github.com/spatie/laravel-csp",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Verhelst",
            "email": "tvke91@gmail.com",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/http": "^6.0",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "~3.9.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Csp\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Csp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Csp\\CspServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
