{
    "name": "spatie/laravel-csp",
    "description": "Add CSP headers to the responses of a Laravel app",
    "keywords": [
        "spatie",
        "laravel-csp",
        "content-security-policy",
        "security",
        "headers",
        "csp",
        "laravel"
    ],
    "homepage": "https://github.com/spatie/laravel-csp",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Verhelst",
            "email": "tvke91@gmail.com",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "illuminate/http": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "spatie/laravel-package-tools": "^1.11"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.3",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "pestphp/pest": "^1.23.0|^2.34.0",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Csp\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Csp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "lint": "php-cs-fixer fix",
        "test": "vendor/bin/pest",
        "test-coverage": "pest --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Csp\\CspServiceProvider"
            ]
        }
    }
}